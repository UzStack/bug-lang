import "math";

class A(){
    func init(){
        this.name = "A";
    }

    func getName(){
        return this.name;
    }
}

class B(){
    func init(){
        this.name = "B";
    }
    func getName(){
        return this.name;
    }
}

class C(B, A){
    func init(){
        super(A).init();
        println(this.name);
        super(B).init();
        println(this.name);
        this.name = "C";
    }
    func getName(){
        return this.name;
    }
}

c = new C();
println(c.getName());

